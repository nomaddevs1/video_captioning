{
  "version": 2,
  "env": {
    "APP_CLIENT_URL": "https://captioning-six.vercel.app",
    "APP_PORT": "8000"
  },
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {"runtime": "python3.8", "maxLambdaSize":"15mb"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.py"
    }
  ]
}
